{
  "name": "abterphp/files",
  "description": "File upload and file sharing module for AbterPHP",
  "authors": [
    {
      "name": "Peter Aba",
      "email": "peter@peteraba.com",
      "role": "Engineer"
    }
  ],
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AbterPhp\\Files\\": [
        "src",
        "tests"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "abterphp/framework": "dev-master",
    "abterphp/admin": "dev-master",
    "abterphp/website": "dev-master",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2",
    "squizlabs/php_codesniffer": "^3.4",
    "phpmd/phpmd": "^2.6",
    "phan/phan": "^2.2"
  },
  "config": {
    "prefer-install": "dist"
  }
}
